{
    "name": "Imbax",
    "description": "The IMBA Shit :]",
    "keywords": [
        "imba",
        "laravel4",
        "angularjs"
    ],
    "require": {
        "laravel/framework": "4.2.*",
        "barryvdh/elfinder-bundle": "dev-master",
        "bkwld/croppa": "~3.0",
        "blueimp/bootstrap-image-gallery": "3.1.0"
    },
    "autoload": {
        "classmap": [
            "app/commands",
            "app/controllers",
            "app/models",
            "app/database/migrations",
            "app/database/seeds",
            "app/tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan optimize"
        ],
        "pre-update-cmd": [
            "php artisan clear-compiled"
        ],
        "post-update-cmd": [
            "php artisan optimize"
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "blueimp/bootstrap-image-gallery",
                "version": "3.1.0",
                "dist": {
                    "url": "https://github.com/blueimp/Bootstrap-Image-Gallery/archive/3.1.0.zip",
                    "type": "zip"
                }
            }
        }
    ]
}
